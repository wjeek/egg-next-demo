{
  "name": "egg-next-boilerplate",
  "version": "0.1.0",
  "description": "egg & next.js boilerplate",
  "private": false,
  "dependencies": {
    "egg": "^2.2.1",
    "egg-scripts": "^2.5.0",
    "immutable": "^3.8.2",
    "isomorphic-unfetch": "^2.0.0",
    "jquery": "^3.3.1",
    "lodash-decorators": "^5.0.1",
    "lost": "^8.2.1",
    "next": "^6.0.3",
    "next-redux-saga": "^3.0.0-beta.1",
    "next-redux-wrapper": "^2.0.0-beta.6",
    "parallax-js": "^3.1.0",
    "postcss-nested": "^3.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "styled-jsx": "^2.2.6",
    "styled-jsx-plugin-postcss": "^0.1.3"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.1.6",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-egg": "^6.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-watch": "^3.1.5",
    "redux-devtools-extension": "^2.13.2",
    "webstorm-disable-index": "^1.2.0"
  },
  "keywords": [
    "egg",
    "next",
    "next.js",
    "react",
    "boilerplate",
    "ssr"
  ],
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-egg-next-boilerplate",
    "stop": "egg-scripts stop --title=egg-server-egg-next-boilerplate",
    "next": "cross-env NODE_ENV=development next",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Gavin1995/egg-next-boilerplate.git"
  },
  "author": "Gavin <gavin199502@gmail.com>",
  "license": "MIT",
  "postcss": {
    "plugins": {
      "lost": {},
      "postcss-nested": {}
    }
  }
}
